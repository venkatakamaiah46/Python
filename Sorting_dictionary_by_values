dict1 = {"Tom":67, "Tina": 54, "Akbar": 87, "Kane": 43, "Divya":73} 
sorted_values = sorted(dict1.values()) # Sort the values 
sorted_dict = {}  

for i in sorted_values:     
    for k in dict1.keys():         
        if dict1[k] == i:             
            sorted_dict[k] = dict1[k]             
            break
print(sorted_dict)
